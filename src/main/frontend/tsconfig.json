{
  "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": false,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "plugins": [
      {
        "name": "typescript-lit-html-plugin"
      }
    ],
    "lib": [
      "dom",
      "dom.iterable"
    ]
  }
}
